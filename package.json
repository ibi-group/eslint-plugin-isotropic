{
    "author": "Steven Olmsted <steven.olmsted@ibigroup.com>",
    "babel": {
        "env": {
            "build": {
                "presets": [
                    "babel-preset-minify"
                ]
            },
            "test": {
                "plugins": [
                    "babel-plugin-istanbul"
                ]
            }
        },
        "plugins": [
            "babel-plugin-transform-line"
        ],
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "sourceMaps": "inline"
    },
    "bugs": "https://github.com/ibi-group/eslint-plugin-isotropic/issues",
    "contributors": [
        "Duane Sibilly <duane.sibilly@ibigroup.com>"
    ],
    "dependencies": {
        "@babel/runtime": "~7.4.4",
        "eslint": "~5.16.0",
        "eslint-config-isotropic": "~0.9.1",
        "eslint-plugin-eslint-comments": "~3.1.1",
        "isotropic-natural-sort": "~0.4.1"
    },
    "description": "ESLint rules for Isotropic",
    "devDependencies": {
        "@babel/cli": "~7.4.4",
        "@babel/core": "~7.4.4",
        "babel-plugin-istanbul": "~5.1.4",
        "babel-plugin-transform-line": "~0.7.1",
        "@babel/preset-env": "~7.4.4",
        "babel-preset-minify": "~0.5.0",
        "@babel/register": "~7.4.4",
        "chai": "~4.2.0",
        "cross-env": "~5.2.0",
        "eslint-find-rules": "~3.3.1",
        "eslint-plugin-eslint-plugin": "~2.0.1",
        "eslint-plugin-self": "~1.2.0",
        "mocha": "~6.1.4",
        "nyc": "~14.1.0"
    },
    "engines": {
        "node": "^10.15.3",
        "npm": "^6.4.1"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "extends": "plugin:self/isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "globalReturn": false,
                "impliedStrict": true,
                "jsx": false
            },
            "ecmaVersion": 2018,
            "sourceType": "module"
        },
        "plugins": [
            "eslint-plugin",
            "self"
        ],
        "root": true,
        "rules": {
            "eslint-plugin/consistent-output": "error",
            "eslint-plugin/fixer-return": "error",
            "eslint-plugin/no-deprecated-context-methods": "error",
            "eslint-plugin/no-deprecated-report-api": "error",
            "eslint-plugin/no-identical-tests": "error",
            "eslint-plugin/no-missing-placeholders": "error",
            "eslint-plugin/no-unused-placeholders": "error",
            "eslint-plugin/no-useless-token-range": "error",
            "eslint-plugin/prefer-output-null": "error",
            "eslint-plugin/prefer-placeholders": "error",
            "eslint-plugin/prefer-replace-text": "error",
            "eslint-plugin/report-message-format": "off",
            "eslint-plugin/require-meta-docs-url": "error",
            "eslint-plugin/require-meta-fixable": "error",
            "eslint-plugin/require-meta-type": "error",
            "eslint-plugin/test-case-property-ordering": "off",
            "eslint-plugin/test-case-shorthand-strings": [
                "error",
                "never"
            ]
        }
    },
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin"
    ],
    "homepage": "https://github.com/ibi-group/eslint-plugin-isotropic",
    "license": "BSD-3-Clause",
    "main": "lib/eslint-plugin-isotropic.js",
    "name": "eslint-plugin-isotropic",
    "nyc": {
        "branches": 100,
        "check-coverage": false,
        "functions": 100,
        "instrument": false,
        "lines": 100,
        "per-file": false,
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "require": [
            "@babel/register"
        ],
        "sourceMap": false,
        "statements": 100
    },
    "repository": "ibi-group/eslint-plugin-isotropic",
    "scripts": {
        "build": "cross-env BABEL_ENV=build babel js -d lib",
        "lint": "npm run build > /dev/null && eslint-find-rules -u package.json && eslint js test",
        "posttest": "[ -z \"$npm_config_coverage\" ] || nyc check-coverage",
        "prepare": "npm run build",
        "prepublishOnly": "npm test --coverage",
        "pretest": "npm run lint",
        "test": "cross-env BABEL_ENV=test nyc mocha"
    },
    "version": "0.4.1"
}